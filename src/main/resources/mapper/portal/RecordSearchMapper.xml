<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="z.note.lite.mapper.portal.RecordSearchMapper">
    <insert id="insert">
        insert into record_search(
            keywords,
            took,
            referer,
            ua,
            ua_name,
            ua_os,
            ip_id,
            channel
        ) values(
            #{keywords},
            #{took},
            #{referer},
            #{ua},
            #{uaName},
            #{uaOs},
            #{ipId},
            #{channel}
        )
    </insert>
</mapper>
